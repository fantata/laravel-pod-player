{
    "name": "fantata/laravel-pod-player",
    "description": "Laravel component to play an audio file, built with podcasts in mind.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fantata\\LaravelPodPlayer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Chris Read",
            "email": "chris@fantata.com"
        }
    ],
    "extra": {
        "laravel": {
